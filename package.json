{
  "name": "jsdbg",
  "version": "0.1.0",
  "description": "A (very) gdb-like debugger frontend for (node.js) JavaScript",
  "main": "lib/index.js",
  "bin": {
    "jsdbg": "lib/main.js"
  },
  "scripts": {
    "jsdbg": "node lib/main.js",
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "gdb",
    "debugger"
  ],
  "author": {
    "name": "Michael Belousov"
  },
  "license": "GPL3",
  "devDependencies": {
    "@types/chrome-remote-interface": "^0.31.9",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.14.6",
    "@types/source-map-support": "^0.5.6",
    "@types/which": "^2.0.2",
    "@types/yargs": "^17.0.22",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chrome-remote-interface": "^0.32.1",
    "debug": "^4.3.4",
    "source-map-support": "^0.5.21",
    "which": "^3.0.0",
    "yargs": "^17.7.1"
  }
}
